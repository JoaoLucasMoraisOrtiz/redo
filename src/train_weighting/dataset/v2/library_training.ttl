    tw:code """function TLibraryManager.FindBook(const AIsbn: string): TBook;
var
  I: Integer;
  Book: TBook;
begin
  Result := nil;
  for I := 0 to FBooks.Count - 1 do
  begin
    Book := TBook(FBooks[I]);
    if SameText(Book.Isbn, AIsbn) then
    begin
      Result := Book;
      Exit;
    end;
  end;
end;""" ;
